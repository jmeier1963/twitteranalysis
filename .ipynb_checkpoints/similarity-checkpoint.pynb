    "from sklearn.metrics.pairwise import cosine_similarity\n",
    "from collections import defaultdict\n",
    "\n",
    "LEVEL = 0.4\n",
    "n = tfidf_vectorizer_vectors.shape[0]\n",
    "\n",
    "def nested_dict(n, type):\n",
    "    if n == 1:\n",
    "        return defaultdict(type)\n",
    "    else:\n",
    "        return defaultdict(lambda: nested_dict(n-1, type))\n",
    "\n",
    "cos_matrix_dict = nested_dict(2,float)\n",
    "        \n",
    "cos_matrix = np.zeros((n,n), dtype=float)\n",
    "for i in range(0, n):\n",
    "    for j in range(i, n):\n",
    "        c = cosine_similarity(tfidf_vectorizer_vectors[i], tfidf_vectorizer_vectors[j], dense_output=True)[0][0]\n",
    "        cos_matrix[i][j] = c\n",
    "        # cos_matrix[j][i] = c\n",
    "        # if c>LEVEL and c<1:\n",
    "        #    print(c, \"\\n\", df_mercator[\"description\"][i], \"\\n\", df_mercator[\"description\"][j], \"\\n\")\n",
    "        \n",
    "print(\"Number of crawling results processed:\", n)"
