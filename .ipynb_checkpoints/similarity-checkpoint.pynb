   ],
   "source": [
    "############################################################################################\n",
    "#\n",
    "# Generate graph for crawled articles\n",
    "#\n",
    "############################################################################################\n",
    "def generate_network(df_sel, labeling):\n",
    "    \"\"\"\n",
    "    Generating a network of articles from pandas dataframe\n",
    "    The cosine similarities for each article are stored in the imported df as a dictionary keyed by id.\n",
    "    \"\"\"\n",
    "    network = nx.Graph()\n",
    "    network.clear()\n",
    "\n",
    "    nodes_dict = {}\n",
    "    nodes=[]\n",
    "                    \n",
    "    # Adding a node for each article\n",
    "    j=0\n",
    "    for i, article in df_sel.iterrows():\n",
    "        if i not in nodes_dict.keys():\n",
    "            nodes_dict[i]=j\n",
    "            ns = 5\n",
    "            if article[\"foundation\"]==\"MD\":\n",
    "                col = \"Blue\"\n",
    "            else:\n",
    "                col = \"Red\"\n",
    "            if labeling==True:\n",
    "                network.add_node(j, node_color=col, node_size=ns,\n",
    "                    node_name=str(i), node_label=article[\"description\"], node_title=article[\"project\"])\n",
    "            else:\n",
    "                network.add_node(j, node_color=col, \n",
    "                    node_size=ns,node_name=\"\", node_label=article[\"description\"], node_title=article[\"project\"])\n",
    "            nodes.append(j)\n",
    "            j+=1\n",
    "    \n",
    "    print(j, \"article nodes added\")\n",
    "    \n",
    "    # Adding edges for similar articles if cosine similarity > 0\n",
    "    n = len(df_sel.index)\n",
    "    c = 0\n",
    "    for i, article in df_sel.iterrows():\n",
    "        for j in range(i+1, n):\n",
    "            value = cos_matrix[i][j]\n",
    "            if value>LEVEL and value<1:\n",
    "                try:\n",
    "                    network.add_edge(nodes_dict[i], nodes_dict[j], edge_color=\"gray\", \\\n",
    "                                     edge_width=3.0, weight = value)\n",
    "                    c+=1\n",
    "                except KeyError:\n",
    "                    print(\"KeyError when trying to add edge for \", article, i)\n",
    "    \n",
    "    print(c, \"similarity edges added\")\n",
    "    return(network, nodes)\n",
    "\n",
    "\n",
    "###########################################################################################\n",
    "#\n",
    "# Initialize layout of network visualization and list of attributes for later use in renderer\n",
    "#\n",
    "###########################################################################################\n",
    "def position_network(layout_type, network, nodes):\n",
    "    \"\"\"\n",
    "    Initialize layout of network visualization and list of attributes for later use in renderer\n",
    "    Takes layout_types random, fruchterman, kamada, spring, spring3D as string input.\n",
    "    Returns layout, Graph, and Nodes ColumnDataSource (for bokeh)\n",
    "    \"\"\"\n",
    "    global node_color, node_size, node_name, node_label, node_title, edge_color, edge_width\n",
    "\n",
    "    node_color = []\n",
    "    node_size = []\n",
    "    node_name = []\n",
    "    node_title = []\n",
    "    node_label = []\n",
    "    edge_color = []\n",
    "    edge_width = []\n",
    "\n",
    "    for n in network.nodes():\n",
    "        node_name.append(network.nodes[n][\"node_name\"])\n",
    "        node_label.append(network.nodes[n][\"node_label\"])\n",
    "        node_color.append(network.nodes[n][\"node_color\"])\n",
    "        node_size.append(network.nodes[n][\"node_size\"])\n",
    "        node_title.append(network.nodes[n][\"node_title\"])\n",
    "\n",
    "    for e in network.edges():\n",
    "        edge_color.append(network.edges[e[0],e[1]][\"edge_color\"])\n",
    "        edge_width.append(network.edges[e[0],e[1]][\"edge_width\"])\n",
    "\n",
    "    if layout_type==\"spring\" or layout_type==\"spring-centrality\":\n",
    "        layout=nx.spring_layout(network, dim=2, k=1.1/sqrt(network.number_of_nodes()), iterations=50)\n",
    "        print(\"spring2D 50 iterations\")\n",
    "    elif layout_type==\"kamada\":\n",
    "        layout=nx.kamada_kawai_layout(network)\n",
    "        print(\"kamada_kawai_layout\")\n",
    "    elif layout_type==\"fruchterman\":\n",
    "        layout=nx.fruchterman_reingold_layout(network)\n",
    "        print(\"fruchterman_reingold_layout\")\n",
    "    elif layout_type==\"random\":\n",
    "        layout=nx.random_layout(network)\n",
    "        print(\"random layout type\")\n",
    "    else:\n",
    "        print(\"non-existent layout\")\n",
    "        layout=nx.random_layout(network)\n",
    "    return(layout)\n",
    "\n",
    "\n",
    "###########################################################################################\n",
    "#\n",
    "# Render 2D network graph with bokeh\n",
    "#\n",
    "###########################################################################################\n",
    "def render_network_bokeh(layout, type_layout, network, filename):\n",
    "    \"\"\"\n",
    "    Takes layout and graph G and renders with bokeh\n",
    "    Takes layout, graph and filename for export\n",
    "    filename is also used as title for graph\n",
    "    \"\"\"\n",
    "\n",
    "    node_size_b=[x**(1./2) for x in node_size]\n",
    "    nodes, nodes_coordinates = zip(*sorted(layout.items()))\n",
    "    nodes_xs, nodes_ys = list(zip(*nodes_coordinates))\n",
    "    nodes_source = ColumnDataSource(dict(x=nodes_xs, y=nodes_ys, node_name=node_name,\n",
    "                                         node_size=node_size_b, node_color=node_color,\n",
    "                                         node_label=node_label, node_title=node_title, \n",
    "                                         node_budget=node_size))\n",
    "\n",
    "    hover = HoverTool(tooltips=[('Name:', '@node_label'),('Title:', '@node_title')],\n",
    "                      show_arrow=False, line_policy='none', point_policy='follow_mouse')\n",
    "\n",
    "    plot = figure(title=filename, plot_width=1000, plot_height=600, sizing_mode='scale_width',\n",
    "                  tools=['tap', hover, 'box_zoom', 'reset'], toolbar_location=\"above\")\n",
    "    plot.axis.visible = False\n",
    "    plot.xgrid.visible = False\n",
    "    plot.ygrid.visible = False\n",
    "\n",
    "    r_circles = plot.circle('x', 'y', source=nodes_source, size='node_size',\n",
    "                            color='node_color', level='overlay', alpha=0.7 )\n",
    "\n",
    "    def get_edges_specs(_network, _layout):\n",
    "        d = dict(xs=[], ys=[], alphas=[])\n",
    "        weights = [data['weight'] for u, v, data in _network.edges(data=True)]\n",
    "        max_weight = max(weights)\n",
    "        calc_alpha = lambda h: 0.1 + 0.6 * (h / max_weight)\n",
    "        for u, v, data in _network.edges(data=True):\n",
    "            d['xs'].append([_layout[u][0], _layout[v][0]])\n",
    "            d['ys'].append([_layout[u][1], _layout[v][1]])\n",
    "            d['alphas'].append(calc_alpha(data['weight']))\n",
    "        return d\n",
    "\n",
    "    lines_source = ColumnDataSource(get_edges_specs(network, layout))\n",
    "    r_lines = plot.multi_line('xs', 'ys', line_width=1.0,\n",
    "                              alpha='alphas', color='navy',\n",
    "                              source=lines_source)\n",
    "\n",
    "    if type_layout==\"spring-centrality\":\n",
    "        centrality = nx.algorithms.centrality.betweenness_centrality(network)\n",
    "        # first element are nodes again\n",
    "        _, nodes_centrality = zip(*sorted(centrality.items()))\n",
    "        max_centraliy = max(nodes_centrality)\n",
    "        nodes_source.add([7 + 10 * t / max_centraliy for t in nodes_centrality], 'centrality')\n",
    "\n",
    "        partition = community.best_partition(network)\n",
    "        p_, nodes_community = zip(*sorted(partition.items()))\n",
    "        nodes_source.add(nodes_community, 'community')\n",
    "        community_colors = ['#e41a1c','#377eb8','#4daf4a','#984ea3','#ff7f00','#ffff33','#a65628', '#b3cde3','#ccebc5','#decbe4','#fed9a6','#ffffcc','#e5d8bd','#fddaec','#1b9e77','#d95f02','#7570b3','#e7298a','#66a61e','#e6ab02','#a6761d','#666666']\n",
    "        nodes_source.add([community_colors[t % len(community_colors)] for t in nodes_community],'community_color')\n",
    "\n",
    "        r_circles.glyph.size = 'centrality'\n",
    "        r_circles.glyph.fill_color = 'community_color'\n",
    "        print(\"Centrality analysis\")\n",
    "\n",
    "    labels = LabelSet(x='x', y='y', text='node_name', level='glyph',\n",
    "                      x_offset=5, y_offset=5, source=nodes_source, render_mode='css',\n",
    "                      text_font_size=\"8pt\")\n",
    "    plot.add_layout(labels)\n",
    "\n",
    "    plot.add_layout(Title(text=\"Network visualization of crawled articles\", align=\"center\"), \"below\")\n",
    "    output_notebook()\n",
    "    # export_png(plot, filename=filename+\".png\")\n",
    "    show(plot)\n",
    "\n",
    "\n",
    "#######################################################################################################\n",
    "#\n",
    "# Function to set up analysis and rendering of network\n",
    "#\n",
    "#######################################################################################################\n",
    "\n",
    "def network_initiative(df, type_layout, labeling, filename):\n",
    "    network, nodes = generate_network(df, labeling)\n",
    "    pos = position_network(type_layout, network, nodes)\n",
    "    return(render_network_bokeh(pos, type_layout, network, filename))\n",
    "\n",
    "print(\"Network functions initialized\")"
   ]
  },
